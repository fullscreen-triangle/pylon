{
  "simulation_metadata": {
    "name": "Sango Rine Shumba Simulation Parameters",
    "description": "Comprehensive simulation parameters for temporal coordination demonstration",
    "version": "1.0",
    "created": "2024-01-01"
  },
  "timing_parameters": {
    "total_duration_seconds": 360,
    "baseline_duration_seconds": 60,
    "sango_duration_seconds": 300,
    "measurement_interval_ms": 10,
    "precision_calculation_interval_ms": 50,
    "fragment_generation_interval_ms": 100,
    "state_prediction_interval_ms": 200
  },
  "network_simulation": {
    "enable_realistic_physics": true,
    "enable_power_grid_interference": true,
    "enable_atmospheric_effects": true,
    "enable_solar_activity_simulation": false,
    "jitter_amplification_factor": 1.0,
    "packet_loss_rate": 0.001,
    "bandwidth_utilization_limit": 0.8,
    "concurrent_message_limit": 1000
  },
  "precision_by_difference": {
    "target_precision_level": "nanosecond",
    "atomic_clock_drift_rate": 1e-15,
    "local_clock_drift_rate": 1e-6,
    "synchronization_accuracy": 1e-9,
    "calculation_window_size": 100,
    "outlier_detection_threshold": 3.0,
    "smoothing_algorithm": "exponential_moving_average",
    "smoothing_factor": 0.1
  },
  "temporal_fragmentation": {
    "min_fragments_per_message": 4,
    "max_fragments_per_message": 16,
    "fragment_size_bytes": 1024,
    "temporal_window_size_ms": 500,
    "fragment_distribution_algorithm": "uniform",
    "entropy_threshold": 0.95,
    "reconstruction_timeout_ms": 2000,
    "security_validation": true
  },
  "mimo_routing": {
    "max_parallel_paths": 4,
    "path_selection_algorithm": "latency_optimized",
    "load_balancing_strategy": "adaptive",
    "convergence_tolerance_ms": 10,
    "retry_attempts": 3,
    "fallback_to_traditional": true,
    "bandwidth_efficiency_target": 1.3
  },
  "state_prediction": {
    "prediction_horizon_ms": 1000,
    "prediction_confidence_threshold": 0.7,
    "user_interaction_models": [
      {
        "name": "web_browsing",
        "click_probability": 0.6,
        "scroll_probability": 0.8,
        "navigation_probability": 0.2,
        "dwell_time_ms": 2000
      },
      {
        "name": "form_interaction", 
        "input_probability": 0.9,
        "submit_probability": 0.3,
        "validation_delay_ms": 500,
        "error_probability": 0.1
      },
      {
        "name": "media_consumption",
        "play_probability": 0.9,
        "pause_probability": 0.4,
        "seek_probability": 0.3,
        "quality_change_probability": 0.1
      }
    ],
    "preemptive_state_cache_size": 10,
    "state_invalidation_time_ms": 5000
  },
  "data_collection": {
    "enable_detailed_logging": true,
    "enable_intermediate_results": true,
    "enable_real_time_metrics": true,
    "metrics_retention_points": 10000,
    "database_batch_size": 100,
    "export_formats": ["json", "csv", "sqlite"],
    "visualization_data_compression": true,
    "performance_profiling": true
  },
  "visualization": {
    "update_interval_ms": 100,
    "max_data_points_displayed": 1000,
    "enable_animations": true,
    "color_schemes": {
      "network_topology": "plasma",
      "precision_metrics": "viridis",
      "performance_comparison": "RdYlBu",
      "fragmentation": "coolwarm"
    },
    "chart_types": {
      "latency": "line_chart",
      "bandwidth": "area_chart",
      "precision": "scatter_plot",
      "topology": "network_graph"
    },
    "export_resolution": {
      "width": 1920,
      "height": 1080,
      "dpi": 300
    }
  },
  "performance_targets": {
    "latency_reduction_target": 0.6,
    "bandwidth_efficiency_target": 1.25,
    "precision_enhancement_target": 3.0,
    "user_experience_improvement_target": 0.5,
    "security_entropy_target": 0.95
  },
  "experimental_conditions": {
    "baseline_measurements": [
      "traditional_tcp_latency",
      "traditional_udp_latency", 
      "http_request_response_time",
      "bandwidth_utilization",
      "packet_loss_rate",
      "jitter_distribution"
    ],
    "sango_measurements": [
      "precision_by_difference_accuracy",
      "temporal_fragmentation_entropy",
      "mimo_routing_efficiency",
      "preemptive_state_hit_rate",
      "collective_coordination_savings",
      "security_reconstruction_resistance"
    ],
    "comparative_analyses": [
      "latency_improvement_distribution",
      "bandwidth_efficiency_comparison",
      "user_experience_metrics",
      "scalability_projections",
      "security_strength_analysis"
    ]
  },
  "test_scenarios": [
    {
      "name": "high_latency_network",
      "description": "Simulate poor network conditions with high jitter",
      "parameters": {
        "jitter_amplification": 5.0,
        "packet_loss_rate": 0.05,
        "bandwidth_limitation": 0.3
      }
    },
    {
      "name": "premium_network",
      "description": "Simulate optimal network conditions", 
      "parameters": {
        "jitter_amplification": 0.2,
        "packet_loss_rate": 0.0001,
        "bandwidth_limitation": 0.9
      }
    },
    {
      "name": "mixed_conditions",
      "description": "Simulate realistic mixed network conditions",
      "parameters": {
        "jitter_amplification": 1.0,
        "packet_loss_rate": 0.001,
        "bandwidth_limitation": 0.7
      }
    },
    {
      "name": "geographic_distance_stress",
      "description": "Emphasize long-distance connections",
      "parameters": {
        "prioritize_distant_connections": true,
        "intercontinental_bias": 0.8,
        "satellite_connection_ratio": 0.4
      }
    }
  ],
  "safety_parameters": {
    "max_cpu_usage_percent": 80,
    "max_memory_usage_mb": 4096,
    "max_disk_usage_mb": 10240,
    "simulation_timeout_seconds": 7200,
    "error_threshold_percent": 5,
    "automatic_shutdown_on_error": false
  },
  "debugging": {
    "enable_verbose_logging": false,
    "enable_packet_tracing": false,
    "enable_timing_analysis": true,
    "enable_memory_profiling": false,
    "log_level": "INFO",
    "debug_components": [
      "precision_calculator",
      "temporal_fragmenter", 
      "mimo_router"
    ]
  }
}
